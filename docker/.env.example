# Australian Grocery Price Database - Environment Configuration
# Copy this file to .env and customize as needed

# InfluxDB Settings
INFLUXDB_PASSWORD=grocerypassword123
INFLUXDB_TOKEN=my-super-secret-grocery-token

# Grafana Settings
GRAFANA_PASSWORD=grocerygrafana123

# Scraper Settings
# How often to write new data to InfluxDB (seconds)
UPDATE_RATE_SECONDS=10

# How old a product can be before it's refreshed (minutes)
# Default: 1440 = 24 hours
MAX_PRODUCT_AGE_MINUTES=1440

# Set to true for verbose logging
DEBUG_LOGGING=false
